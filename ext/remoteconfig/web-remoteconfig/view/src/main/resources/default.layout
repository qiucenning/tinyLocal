<!--[if (gte IE 9)|!(IE)]><!--><html class="no-js" lang="zh-cmn-Hans-CN"> <!--<![endif]-->
<html lang="zh-cmn-Hans-CN">
<head>
	<meta charset="utf-8"/>
	<meta name="renderer" content="webkit|ie-comp|ie-stand">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
	<meta http-equiv="Cache-Control" content="no-siteapp" />
	<title>
		#if(activeTitle)${activeTitle} #{end}Tiny分布式配置
	</title>
	<meta name="keywords" content="Tiny分布式配置">
	<script>
		var contextPath = "${TINY_CONTEXT_PATH}";#if(UPLOAD_ACTION_URL)
		var UPLOAD_ACTION_URL = '${TINY_CONTEXT_PATH}${UPLOAD_ACTION_URL}'; #end
	</script>
	<link rel="shortcut icon" href="${TINY_CONTEXT_PATH}/img/tiny32.png">
	#if(DEBUG_MODE && DEBUG_MODE=="true")
		#foreach(component in uiengine.getHealthUiComponents())
			#if(component.cssResource)#set(resources=component.cssResource.split(","))
				#foreach(path in resources)#set(path=path.trim(),newPath=path.replaceAll("[$][{]TINY_THEME[}]","${TINY_THEME}"))
					<link href="${TINY_CONTEXT_PATH}${newPath}" rel="stylesheet" />
				#end
			#end
			#if(component.jsResource)
				#set(resources=component.jsResource.split(","))
				#foreach(path in resources)#set(path=path.trim())
					<script src="${TINY_CONTEXT_PATH}${path}"></script>
				#end
			#end
			#if(component.jsCodelet)
				<script>
				$!{component.jsCodelet}
				</script>
							#end
							#if(component.cssCodelet)
								<style>
				$!{component.cssCodelet}
				</style>
			#end
			#if(component.compatibilityResources)
				#set(resources=component.compatibilityResources.getCompatibilityResources())
				#foreach(compatibilityResource in resources)
					#if(compatibilityResource.condition)
							<!--[if ${compatibilityResource.condition}]>
							#if(compatibilityResource.cssResource)
								#set(csslist=compatibilityResource.cssResource.split(","))
								#foreach(path in csslist)
									#set(path=path.trim())
									#set(newPath=path.replaceAll("[$][{]TINY_THEME[}]","${TINY_THEME}"))
								<link href="${TINY_CONTEXT_PATH}${newPath}" rel="stylesheet" />
								#end
							#end
							#if(compatibilityResource.jsResource)
								#set(jslist=compatibilityResource.jsResource.split(","))
								#foreach(path in jslist)
									#set(path=path.trim())
								<script src="${TINY_CONTEXT_PATH}${path}"></script>
								#end
							#end
						<![endif]-->
					#end
				#end
			#end

		#end
	#else
		<!--[if lt ie 9]>
			#foreach(component in uiengine.getHealthUiComponents())
				#if(component.cssResource)
					#set(resources=component.cssResource.split(","))
					#foreach(path in resources)
						#set(path=path.trim())
						#set(newPath=path.replaceAll("[$][{]TINY_THEME[}]","${TINY_THEME}"))
					<link href="${TINY_CONTEXT_PATH}${newPath}" rel="stylesheet" />
					#end
				#end
				#if(component.cssCodelet)
				<style>
						$!{component.cssCodelet}
				</style>
				#end
			#end
		<![endif]-->
		<!--[if (gte IE 9)|!(IE)]><!-->
		<link href="${TINY_CONTEXT_PATH}/uiengine.uicss" rel="stylesheet" />
		<!--<![endif]-->
		<script src="${TINY_CONTEXT_PATH}/uiengine.uijs"></script>
	#end
	<style type="text/css">#main-navbar .navbar-brand img{margin-top:3px}</style>
</head>
<body class="main-menu-animated theme-clean menu-on-top">
	#@mainWrapper()
		#@mainNavbar("Tiny")
			#@rightNavBar()
			#*<li class="nav-icon-btn nav-icon-btn-danger dropdown">
				#@notifyDropdown(5 "fa-bullhorn" "通知" "更多的通知")
				#include("/page/index/notify.page")
			#end
			</li>
			<li class="nav-icon-btn nav-icon-btn-success dropdown">
				#@messagesDropdown(12 "fa-envelope" "所得的消息" "更多的信息")
					#include("/page/index/messages.page")
			#end
			</li>*#
			<li class="fullscreen">
				<a href="javascript:void(0);" onclick="launchFullScreen(document.documentElement);" title="全屏">#faIcon("fullscreen")</a>
			</li>

			#end
		#end
	<div class="content-wrapper-bg"></div>
		<div id="content-wrapper" style="padding-top:22px;padding-left:8px;padding-right:8px">
			<!-- 主体 start -->
			#@rowFrame()
				${pageMenuList}
				<div id="tinypagecontent" style="min-height:490px;padding-top: 30px;">
				${pageContent}
            </div>
			#end
			<div id="tinyfooter">
				<div class="tinyfooterrap">
				<span class="right">
					<a class="record-num" target="_blank" href="http://www.beianbeian.com/search/tinygroup.org">浙ICP备14043110号-1</a>
				</span>
				TinyGroup官方网站 &copy; 2013-${formatDate(now(),"yyyy")}</div>
			</div>
		</div>
	#end
	</body>
</html>