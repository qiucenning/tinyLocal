<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
                    http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
                    http://www.springframework.org/schema/context
                    http://www.springframework.org/schema/context/spring-context-2.5.xsd"
	default-autowire="byName" default-lazy-init="true">

	<bean id="caseModelServiceProcessor" name="caseModelServiceProcessor"
		scope="prototype" class="org.tinygroup.imda.usermodel.CaseModelServiceProcessor">
	</bean>
	<bean id="caseModelViewProcessor" name="caseModelViewProcessor"
		scope="prototype" class="org.tinygroup.imda.usermodel.CaseModelViewProcessor">
	</bean>
	<bean id="parameterBuilder" name="parameterBuilder" scope="prototype"
		class="org.tinygroup.imda.usermodel.CaseModelParamBuilder">
	</bean>
	<bean id="infoGetter" name="infoGetter" scope="prototype"
		class="org.tinygroup.imda.usermodel.InfoGetter">
	</bean>
	<bean id="baseModelLoader" name="baseModelLoader" scope="prototype"
		class="org.tinygroup.imda.usermodel.BaseModelLoader">
	</bean>
	
</beans>
