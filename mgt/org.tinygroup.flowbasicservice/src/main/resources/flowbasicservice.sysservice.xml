<service-components>
	<service-component
		type="org.tinygroup.flowbasicservice.FlowService" group-id="org.tinygroup.flowbasicservice"
		artifact-id="FlowService">

		<service-method name="getTinyFlows" local-name="getTinyFlows"
			service-id="getTinyFlows" version="" description="获取Tiny所有的逻辑流"
			method-name="getFlows">
			<service-parameters>
			</service-parameters>
			<service-result name="flows" required="true" is-array="false"
				type="org.tinygroup.flow.config.Flow" collection-type="java.util.ArrayList" />
		</service-method>

		<service-method name="getTinyFlow" local-name="getTinyFlow"
			service-id="getTinyFlow" version="" description="根据id获取指定的Tiny逻辑流"
			method-name="getFlow">
			<service-parameters>
				<service-parameter name="id" type="java.lang.String"
					required="true" is-array="false" />
			</service-parameters>
			<service-result name="flow" required="true" is-array="false"
				type="org.tinygroup.flow.config.Flow" />
		</service-method>

	</service-component>
</service-components>