<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
                    http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
                    http://www.springframework.org/schema/context
                    http://www.springframework.org/schema/context/spring-context-2.5.xsd"
	default-autowire="byName" default-lazy-init="true">

	<bean id="context2ObjectComponent" name="context2ObjectComponent"
		scope="prototype" class="org.tinygroup.flowbasiccomponent.Context2Object">
	</bean>
	<bean id="queryAllFlowComponent" name="queryAllFlowComponent"
		scope="prototype" class="org.tinygroup.flowbasiccomponent.QueryAllFlow">
	</bean>
	<bean id="callMethodComponent" name="callMethodComponent" scope="prototype"
		class="org.tinygroup.flowbasiccomponent.CallMethod">
	</bean>
	<bean id="keyChangeComponent" name="keyChangeComponent" scope="prototype"
		class="org.tinygroup.flowbasiccomponent.KeyChange">
	</bean>
	<bean id="callElComponent" name="callElComponent" scope="prototype"
		class="org.tinygroup.flowbasiccomponent.CallEl">
	</bean>
</beans>
