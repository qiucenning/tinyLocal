<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-2.5.xsd"
       default-autowire="byName" default-lazy-init="true">
	<bean id="validateAddress" name="validateAddress" scope="prototype"
		class="org.tinygroup.cepcore.test.aop.bean.AopValidateAddress">
	</bean>
	<bean id="validateUser" name="validateUser" scope="prototype"
		class="org.tinygroup.cepcore.test.aop.bean.AopValidateUser">
	</bean>
	<bean id="validateService" name="validateService" scope="prototype"
		class="org.tinygroup.cepcore.test.aop.bean.AopValidateService">
	</bean>
	<bean id="exceptionService" name="exceptionService" scope="prototype"
		class="org.tinygroup.cepcore.test.aop.exception.ExceptionService">
	</bean>
</beans>