<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="dialect-function-mappings">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="dialect-function" minOccurs="0"
					maxOccurs="unbounded">
					<xs:complexType>
						<xs:all>
							<xs:element name="dialects" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="dialect" minOccurs="0" maxOccurs="unbounded">
											<xs:complexType>
												<xs:attribute name="name" type="xs:string" />
												<xs:attribute name="function-name" type="xs:string" />
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:all>
						<xs:attribute name="format" type="xs:string" />
						<xs:attribute name="desc" type="xs:string" />
						<xs:attribute name="name" type="xs:string" />
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>