<standard-types package-name="org.tinygroup.demo">
	<standard-type type="TinyText" id="type_tiny_text">
		<dialect-type-list>
			<dialect-type language="oracle" base-type="VARCHAR2" ext-type="(255)"
				default-value="">
			</dialect-type>
			<dialect-type language="mysql" base-type="TINYTEXT"
				default-value="">
			</dialect-type>
			<dialect-type language="java" base-type="String"
				default-value="">
			</dialect-type>
		</dialect-type-list>
	</standard-type>
	<standard-type type="Text" id="type_text">
		<dialect-type-list>
