		<placeholder-list>
			<placeholder name="length" title="长度">
				<description>字符串的长度</description>
			</placeholder>
		</placeholder-list>
		<dialect-type-list>
			<dialect-type language="oracle" base-type="CHAR" ext-type="(${length})"
